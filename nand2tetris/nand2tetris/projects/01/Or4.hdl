 /**
 * Or4 gate:
 * out = 0 if (a == 0 and b == 0 and c == 0 and d == 0)
 *       1 otherwise
 */

CHIP Or4 {
    IN a, b,c,d;
    OUT out;

    PARTS:
    Or3(a,b,c,out=aORbORc);
    Or (a=d, b =aORbORc, out=out);    
}
